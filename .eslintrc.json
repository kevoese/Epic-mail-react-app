{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-mixed-spaces-and-tabs": "off",
    "no-undef": "off",
    "no-unused-expressions": 0,
    "linebreak-style": ["error", "unix"],
    "react/prop-types": "off",
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}