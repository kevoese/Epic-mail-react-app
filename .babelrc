{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@": "./",
					"@components": "./src/components",
					"@pages": "./src/pages"
				}
			}
		],
		"@babel/plugin-proposal-class-properties"
	],
	"ignore": ["./dist"]
}
