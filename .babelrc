{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@": "./",
					"@components": "./src/components",
					"@pages": "./src/pages",
					"@images": "./src/images",
					"@utils": "./src/utils",
					"@actions": "./src/store/actions",
					"@reducers": "./src/store/reducers"
				}
			}
		],
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-runtime"
	],
	"ignore": ["./dist"]
}
